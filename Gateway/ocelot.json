{
  "Routes": [
    // Identity Service
    {
      "DownstreamPathTemplate": "/api/Auth/login",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 44328
        }
      ],
      "UpstreamPathTemplate": "/gateway/login",
      "UpstreamHttpMethod": [ "POST" ],
      "SwaggerKey": "auth",
      "Auth": false
    },

    {
      "DownstreamPathTemplate": "/api/Auth/get-user",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 44328
        }
      ],
      "UpstreamPathTemplate": "/gateway/get-user",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "auth"
    },

    // Room Booking Service
    {
      "DownstreamPathTemplate": "/api/Rooms",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 44366
        }
      ],
      "UpstreamPathTemplate": "/gateway/Rooms",
      "UpstreamHttpMethod": [ "GET", "POST" ],
      "SwaggerKey": "room"
    },
    {
      "DownstreamPathTemplate": "/api/Rooms/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 44366
        }
      ],
      "UpstreamPathTemplate": "/gateway/Rooms/{id}",
      "UpstreamHttpMethod": [ "GET", "DELETE" ],
      "SwaggerKey": "room"
    },
    {
      "DownstreamPathTemplate": "/api/Bookings",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 44366
        }
      ],
      "UpstreamPathTemplate": "/gateway/Bookings",
      "UpstreamHttpMethod": [ "GET", "POST" ],
      "SwaggerKey": "booking",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "TestKey",
        "AllowedScopes": []
      }
    },
    {
      "DownstreamPathTemplate": "/api/Bookings/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 44366
        }
      ],
      "UpstreamPathTemplate": "/gateway/Bookings/{id}",
      "UpstreamHttpMethod": [ "GET", "PUT", "DELETE" ],
      "SwaggerKey": "booking"
    },
    {
      "DownstreamPathTemplate": "/api/Bookings/test",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 44366
        }
      ],
      "UpstreamPathTemplate": "/gateway/Bookings/test",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "booking"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "auth",
      "Config": [
        {
          "Name": "Auth API",
          "Version": "v1",
          "Url": "https://localhost:44328/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "room",
      "Config": [
        {
          "Name": "Room API",
          "Version": "v1",
          "Url": "https://localhost:44366/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "booking",
      "Config": [
        {
          "Name": "Booking API",
          "Version": "v1",
          "Url": "https://localhost:44366/swagger/v1/swagger.json"
        }
      ]
    }
  ]
