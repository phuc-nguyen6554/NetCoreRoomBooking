{
  "Routes": [
    {
      "DownstreamPathTemplate": "/{everything}", /* api/leave-request */
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 2002
        }
      ],
      "UpstreamPathTemplate": "/leaves/{everything}", /* PUT => /leave/api/leave-request/1 */
      "UpstreamHttpMethod": [ "GET", "POST", "PUT" ],
      "SwaggerKey": "leaving"
    },
    // Identity Service
    {
      "DownstreamPathTemplate": "/api/Auth/login",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 2001
        }
      ],
      "UpstreamPathTemplate": "/login",
      "UpstreamHttpMethod": [ "POST" ],
      "SwaggerKey": "auth",
      "Auth": false
    },

    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 2001
        }
      ],
      "UpstreamPathTemplate": "/identities/{everything}",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "auth"
    },

    // Room Booking Service
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 2003
        }
      ],
      "UpstreamPathTemplate": "/bookings/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST" ],
      "SwaggerKey": "room"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "auth",
      "Config": [
        {
          "Name": "Auth API",
          "Version": "v1",
          "Url": "https://localhost:44328/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "room",
      "Config": [
        {
          "Name": "Room API",
          "Version": "v1",
          "Url": "https://localhost:44366/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "booking",
      "Config": [
        {
          "Name": "Booking API",
          "Version": "v1",
          "Url": "https://localhost:44366/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "leave",
      "Config": [
        {
          "Name": "Leave Request API",
          "Version": "v1",
          "Url": "https://localhost:44366/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}